QMessageBox {
    background-color: #F5F5F5;
    color: #333333;
}

QMessageBox QPushButton {
    background-color: #F5F5F5;
    color: #333333;
    padding: 5px 15px;
    border-radius: 3px;
    border: 1px solid #CCCCCC;
    min-width: 75px;
}

QMessageBox QPushButton:hover {
    background-color: #E0E0E0;
    border: 1px solid #B5B5B5;
}